{
  "name": "tree-sitter-mermaid",
  "version": "0.25.0",
  "description": "Mermaid.js grammar for tree-sitter",
  "repository": "https://github.com/mglst/tree-sitter-mermaid",
  "license": "MIT",
  "author": "monaqa",
  "contributors": [
    "Singularity",
    {
      "name": "Maximilien Tirard",
      "email": "maximilien.tirard@gmail.com"
    }
  ],
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "mermaid"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "eslint": "^10.0.1",
    "eslint-config-treesitter": "^1.0.2",
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.26.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.26.0"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "lint": "eslint grammar.js",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  }
}