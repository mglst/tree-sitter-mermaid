{
  "name": "tree-sitter-mermaid",
  "version": "0.25.0",
  "description": "Mermaid.js grammar for tree-sitter",
  "repository": "https://github.com/mglst/tree-sitter-mermaid",
  "license": "MIT",
  "author": "monaqa",
  "contributors": [
    {
      "name": "Mikael Hugo",
      "url": "https://github.com/mikkihugo"
    },
    {
      "name": "Maximilien Tirard",
      "email": "maximilien.tirard@gmail.com"
    }
  ],
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "mermaid"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "peerDependencies": {
    "tree-sitter": "^0.25.0"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "lint": "eslint grammar.js",
    "prebuildify": "npm_config_loglevel=warn prebuildify --napi --strip",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  }
}